{
  "name": "react-postcss-parcel-app",
  "version": "1.0.0",
  "description": "React, PostCSS and Parcel Bundler app",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">=9.x.x",
    "npm": ">=5.x.x",
    "yarn": ">=1.x.x"
  },
  "browserslist": [
    "Firefox >= 60",
    "Chrome >= 65",
    "ie >= 11",
    "last 4 versions",
    "Safari >= 12"
  ],
  "main": "src/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development parcel src/index.html --no-cache -p 1337",
    "start-open": "cross-env NODE_ENV=development parcel src/index.html --no-cache -p 1337 --open",
    "build": "cross-env NODE_ENV=production parcel build src/index.html --no-minify --public-url /"
  },
  "dependencies": {
    "@ant-design/icons": "^4.0.6",
    "@babel/runtime": "^7.7.2",
    "antd": "^4.1.5",
    "axios": "^0.19.0",
    "immer": ">=2.0.0",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0",
    "react-router-dom": "^5.1.2",
    "use-immer": "^0.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.6.1",
    "babel-plugin-import": "^1.12.2",
    "cross-env": "^5.2.1",
    "less": "^3.10.3",
    "parcel-bundler": "^1.12.3",
    "postcss-modules": "^1.4.1",
    "sass": "^1.22.12"
  }
}
